    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/node": "^13.7.4",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
