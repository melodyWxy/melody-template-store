{
  "name": "{{name}}",
  "version": "1.0.0",
  "main": "",
  "scripts": {
    "dev": "cross-env NODE_ENV='development' webpack-dev-server --config ./config/webpack.dev.js --colors",
    "build": "cross-env NODE_ENV='production' webpack --config ./config/webpack.prod.js",
    "commit": "git-cz",
    "lint:script": "eslint --ext .tsx,.ts,.js,.jsx --fix ./src ./config",
		"lint:style": "stylelint --fix 'src/**/*.{less,css}' --syntax less",
		"lint": "yarn run lint:style && yarn run lint:script"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{json,md,yml}": "prettier --write",
    "*.{less,css}": [
      "yarn run lint:style"
    ],
    "*.{tsx,ts,js,jsx}": [
      "yarn run lint:script"
    ]
  },
  "sideEffects": [
    "dist/*",
    "*.less",
    "*.css"
  ],
  "author": "{{author}}",
  "license": "ISC",
  "description": "{{description}}",
  "dependencies": {
    "@tuya-fe/cdn": "^0.0.4",
    "antd": "^4.0.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.30",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5"
  },
